.data 
quebra_linha: .asciiz "\n"
.text
main: 
    li		$v0,     5 		# Read Int
    syscall
    move 	$t1, $v0		# $t1= $v0

    li		$v0,     5 		# Read Int
    syscall
    move 	$t2, $v0		# $t2= $v0

    add $t0, $t1,$t2        # $t0 = $t1+$t2

    li      $v0,    1       # system call - Print int
    move    $a0, $t0
    syscall

    li      $v0,    4       # system call - Print string
    la      $a0, quebra_linha
    syscall

    li      $v0, 10
    syscall
